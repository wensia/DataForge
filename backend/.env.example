# 应用配置
APP_NAME=云客中转
APP_VERSION=0.1.0
DEBUG=true

# 数据库配置
DATABASE_URL=sqlite:///./app.db

# API 配置
API_PREFIX=/api/v1

# ========== API密钥认证配置 ==========
# API密钥列表(逗号分隔,每个密钥至少32位)
# 生成密钥示例: python -c "import secrets; print('yk_' + secrets.token_urlsafe(32))"
API_KEYS=yk_example_key_1_xxxxxxxxxxxxxxxxxxxxxxxx,yk_example_key_2_yyyyyyyyyyyyyyyyyyyyyyyy

# 安全审计配置
ENABLE_SECURITY_AUDIT=true
SECURITY_LOG_FILE=logs/security.log
