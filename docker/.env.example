# ========================================
# DataForge Docker 环境变量配置
# 复制此文件为 .env 并修改配置
# ========================================

# ===== PostgreSQL 配置 =====
POSTGRES_PASSWORD=your_secure_postgres_password
POSTGRES_DB=dataforge

# ===== Redis 配置 =====
REDIS_PASSWORD=your_secure_redis_password

# ===== 后端应用配置 =====
# 基础配置
APP_NAME=DataForge
APP_VERSION=0.1.0
DEBUG=false

# API 配置
API_PREFIX=/api/v1
API_KEYS=df_your_secure_api_key_here

# JWT 配置
JWT_SECRET_KEY=your_super_secure_jwt_secret_key_change_this
JWT_EXPIRE_HOURS=24

# 调度器配置
SCHEDULER_ENABLED=true
TIMEZONE=Asia/Shanghai
MAX_EXECUTION_HISTORY_DAYS=30

# 安全审计
ENABLE_SECURITY_AUDIT=true
SECURITY_LOG_FILE=logs/security.log

# ===== AI 服务配置 =====
KIMI_API_KEY=your-kimi-api-key
DEEPSEEK_API_KEY=your-deepseek-api-key
DEFAULT_AI_PROVIDER=kimi

# ===== 缓存配置 =====
RECORD_CACHE_TTL=180
API_KEY_CACHE_TTL=300

# ===== 前端构建配置 =====
VITE_API_BASE_URL=/api/v1
